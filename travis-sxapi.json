{
    "name": "sxapi-default-travis",
    "description": "sxapi default travis config file",
    "version": "0.1.0",
    "debug": true,
    "log": {
        "filters": {
            "level": "0,1,2",
            "type": "debug,info,error,warn"
        }
    },
    "server": {
        "port": "8080",
        "endpoints": [
            {
                "path": "/",
                "desc": "Display welcome message (HTML)",
                "body": "<html><head><title>sxapi-core API</title></head><body><h1>Your API is live !</h1><p>This API is based on sxapi-core v0.1.0 and use default config file. You can now start to configure your sxapi.json config file in order to build your own API </p></body></html>"
            },
            {
                "path": "/ping",
                "desc": "Ping the application. Return a sample message in json",
                "code": "200",
                "body": "pong"
            }
        ]
    }
}
