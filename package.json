{
  "name": "sxapi-core",
  "version": "0.3.9",
  "description": "sxapi-core is an simple and extensible framework for building microservices API using javascript technologies",
  "author": "STARTX <dev+sxapi_core@startx.fr>",
  "homepage": "https://github.com/startxfr/sxapi-core",
  "license": "GPL-3.0",
  "private": false,
  "analyze": false,
  "contributors": [
    {
      "name": "startx-dev",
      "email": "dev@startx.fr"
    },
    {
      "name": "mallowtek",
      "email": "mallowtek@gmail.com"
    }
  ],
  "keywords": [
    "sxapi",
    "startx",
    "microservice",
    "nodejs",
    "npm-module",
    "simple",
    "lightweight",
    "extensible",
    "FaaS"
  ],
  "main": "app.js",
  "directories": {
    "lib": "core",
    "doc": "docs",
    "test": "test",
    "api-examples": "api-examples"
  },
  "scripts": {
    "dev": "npm-run-all --parallel \"dev:internal:live-reload\" \"dev:start\" \"dev:internal:open\"",
    "dev:env": "env",
    "dev:clean": "rimraf package-lock.json",
    "dev:install": "npm install --only=dev -D && npm install --only=prod -P && npm audit fix --force && npm dedupe",
    "dev:build": "npm run dev:env -s && npm run dev:clean -s && npm run dev:install -s && npm update --save && npm dedupe ",
    "dev:start": "npm run dev:install && npm test && npm start",
    "dev:internal:open": "wait-on http://localhost:8077 && echo 'Application is running, open your browser to http://localhost:8077'",
    "dev:internal:live-reload": "live-reload --port=8078 --delay=2000 ./",
    "test": "./node_modules/.bin/mocha --exit",
    "prod": "npm run prod:build && npm run test && npm run prod:start",
    "prod:clean": "rimraf package-lock.json *.md docs test",
    "prod:install": "npm install --only=prod -P && npm update --no-save && npm audit fix --force && npm dedupe",
    "prod:build": "npm install rimraf --no-save && npm run prod:clean -s && npm run prod:install -s",
    "prod:start": "npm start",
    "build": "npm run prod:build -s",
    "start": "node app.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/startxfr/sxapi-core.git"
  },
  "bugs": {
    "url": "https://github.com/startxfr/sxapi-core/issues"
  },
  "dependencies": {
    "merge": "~1.2.1",
    "md5": "~2.2.1",
    "glob": "~7.1.4",
    "async": "~3.1.0",
    "change-case": "~3.1.0",
    "mustache": "~3.0.1",
    "moment": "~2.24.0",
    "timer-machine": "~1.1.0",
    "uuid": "~3.3.2",
    "node-cron": "~2.0.3",
    "ip": "~1.1.5",
    "request": "~2.88.0",
    "express": "~4.17.1",
    "cors": "~2.8.5",
    "body-parser": "~1.19.0",
    "cookies": "~0.7.3",
    "aws-sdk": "~2.501.0",
    "mysql": "~2.17.1",
    "googleapis": "~41.0.1",
    "google-auth-library": "~5.1.0",
    "busboy": "~0.3.0",
    "redis": "~2.8.0",
    "pg": "~7.12.0",
    "memcache": "~0.3.0",
    "couchbase": "~2.6.5",
    "nodemailer": "~6.3.0",
    "siren2tva": "~1.0.0",
    "twitter": "~1.7.1",
    "object-recursive-iterator": "~1.0.2",
    "socket.io": "~2.2.0",
    "socket.io-client": "~2.2.0",
    "mime-types": "~2.1.24",
    "js-yaml": "~3.13.1"
  },
  "devDependencies": {
    "mocha": "~6.2.0",
    "chai": "~4.2.0",
    "chai-http": "~4.3.0",
    "sinon": "~7.3.2",
    "http-server": "^0.11.1",
    "jshint": "^2.10.2",
    "live-reload": "^1.1.0",
    "nodemon": "^1.19.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.6.3",
    "wait-on": "^3.3.0"
  },
  "engines": {
    "node": ">= 11.7.0",
    "npm": ">= 6.5.0"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ]
}
