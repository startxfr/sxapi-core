{
  "name": "sxapi-core",
  "version": "0.3.46",
  "description": "sxapi-core is an simple and extensible framework for building microservices API using javascript technologies",
  "author": "STARTX <dev+sxapi_core@startx.fr>",
  "homepage": "https://github.com/startxfr/sxapi-core",
  "license": "GPL-3.0",
  "private": false,
  "analyze": true,
  "contributors": [
    {
      "name": "startx-dev",
      "email": "dev@startx.fr"
    },
    {
      "name": "mallowtek",
      "email": "mallowtek@gmail.com"
    }
  ],
  "keywords": [
    "sxapi",
    "startx",
    "microservice",
    "nodejs",
    "npm-module",
    "simple",
    "lightweight",
    "extensible",
    "FaaS"
  ],
  "main": "app.js",
  "directories": {
    "lib": "core",
    "doc": "docs",
    "test": "test",
    "examples": "examples"
  },
  "scripts": {
    "dev": "npm-run-all --parallel \"dev:internal:live-reload\" \"dev:start\" \"dev:internal:open\"",
    "dev:env": "env",
    "dev:clean": "rm -rf package-lock.json *.md docs",
    "dev:install": "npm install --only=dev --no-audit -D && npm install --no-audit --only=prod -P && npm audit fix --force && npm dedupe",
    "dev:build": "npm run dev:env -s && npm run dev:clean -s && npm run dev:install -s && npm update --save && npm audit fix --force && npm dedupe",
    "dev:start": "npm run dev:install && npm test && npm start",
    "dev:internal:open": "wait-on http://localhost:8077 && echo 'Application is running, open your browser to http://localhost:8077'",
    "dev:internal:live-reload": "live-reload --port=8078 --delay=2000 ./",
    "test": "./node_modules/.bin/mocha --exit",
    "prod": "npm run prod:build && npm run prod:start",
    "prod:clean": "rm -rf package-lock.json *.md docs test",
    "prod:install": "npm install --only=prod --no-audit -P && npm update --no-audit --no-save && npm audit fix --force && npm dedupe",
    "prod:build": "npm run prod:clean -s && npm run prod:install -s",
    "prod:start": "npm start",
    "build": "npm run prod:build -s",
    "start": "node app.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/startxfr/sxapi-core.git"
  },
  "bugs": {
    "url": "https://github.com/startxfr/sxapi-core/issues"
  },
  "dependencies": {
    "async": "^3.2.0",
    "aws-sdk": "~2.501.0",
    "body-parser": "~1.19.0",
    "busboy": "^0.3.1",
    "change-case": "~3.1.0",
    "cookies": "~0.7.3",
    "cors": "~2.8.5",
    "express": "~4.17.1",
    "glob": "^7.1.6",
    "google-auth-library": "^5.10.1",
    "googleapis": "~41.0.1",
    "ip": "~1.1.5",
    "js-yaml": "~3.13.1",
    "md5": "~2.2.1",
    "memcache": "~0.3.0",
    "merge": "~1.2.1",
    "mime-types": "^2.1.27",
    "moment": "~2.24.0",
    "mustache": "^3.2.1",
    "mysql": "~2.17.1",
    "node-cron": "~2.0.3",
    "nodemailer": "^6.4.8",
    "object-recursive-iterator": "~1.0.2",
    "pg": "^7.18.2",
    "redis": "~2.8.0",
    "request": "^2.88.2",
    "siren2tva": "~1.0.0",
    "socket.io": "~2.2.0",
    "socket.io-client": "~2.2.0",
    "timer-machine": "~1.1.0",
    "twitter": "~1.7.1",
    "uuid": "^3.4.0",
    "fsevents": "^2.1.3",
    "couchbase": "^3.0.2"
  },
  "devDependencies": {
    "chai": "~4.2.0",
    "chai-http": "~4.2.1",
    "http-server": "^0.11.1",
    "jshint": "^2.11.1",
    "live-reload": "^1.1.0",
    "mocha": "~5.2.0",
    "nodemon": "^1.19.4",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.7.1",
    "sinon": "~7.2.3",
    "wait-on": "^3.3.0"
  },
  "engines": {
    "node": ">= 11.7.0",
    "npm": ">= 6.5.0"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ]
}
